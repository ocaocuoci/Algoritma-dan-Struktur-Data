System.out.println("Data yang sudah terurut menggunakan INSERTION SORT (ASC): ");
        list.insertionSort();
        list.tampil();

        void insertionSort(){
        for(int i=1; i<listMhs.length; i++){
            mahasiswa12 temp = listMhs[i];
            int j=i;
            while(j<0 && listMhs[j-1].ipk>temp.ipk){
                listMhs[j]=listMhs[j-1];
                j--;
            }
            listMhs[j]=temp;
        }
    }